#include<stdio.h>
void isort(int arr[],int n)
{
int i,j,key;
for(i=1;i<n;i++)
{
key=arr[i];
j=i-1;
while(j>=0 && arr[j]>key)
{
arr[j+1]==arr[j];
j=j-1;
}
arr[j+1]=key;
}}
void printarray(int arr[], int n)
{
int i;
for(i=0;i<n;i++)
printf("%d",arr[i]);
}
int main()
{
int arr[]={1,30,77,25,23};
int n=sizeofarr()/sizeof(arr[0]);
insertsort(arr,n);
printarray(arr,n);
return 0;
}

